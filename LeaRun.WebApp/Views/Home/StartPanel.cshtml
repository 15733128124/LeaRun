                        '7月份', '8月份', '9月份', '10月份', '11月份', '12月份']
                },
                yAxis: {
                    title: {
                        text: '销售产品'
                    },
                    labels: {
                        formatter: function () {
                            return this.value + '°'
                        }
                    }
                },
                tooltip: {
                    crosshairs: true,
                    shared: true
                },
                plotOptions: {
                    spline: {
                        marker: {
                            radius: 4,
