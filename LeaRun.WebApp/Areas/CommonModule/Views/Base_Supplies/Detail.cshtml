@{
    ViewBag.Title = "供应商表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/Base_Supplies/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>供应商编码：</th>
            <td class='formValue'>
                <label id="Code" />
            </td>
            <th class='formTitle'>供应商名称：</th>
            <td class='formValue'>
                <label id="FullName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>电话：</th>
            <td class='formValue'>
                <label id="Telephone" />
            </td>
            <th class='formTitle'>传真：</th>
            <td class='formValue'>
                <label id="Fax" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>结算方式：</th>
            <td class='formValue'>
                <label id="AccountsMethod" />
            </td>
            <th class='formTitle'>联系人：</th>
            <td class='formValue'>
                <label id="LinkMan" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>负责人：</th>
            <td class='formValue'>
                <label id="LeadingOfficialId" />
            </td>
            <th class='formTitle'>地址：</th>
            <td class='formValue'>
                <label id="Address" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>
