@{
    ViewBag.Title = "学生公告";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/DStudent_Announcement/EditDAnnouncement", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="ID" />
            </td>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="Title" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="Content" />
            </td>
            <th class='formTitle'>：</th>
