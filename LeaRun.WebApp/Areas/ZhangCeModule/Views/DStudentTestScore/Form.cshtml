@{
    ViewBag.Title = "学生成绩测试";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script src="~/Content/layer/layer.js"></script>
<link href="~/Content/Styles/dstudenttestscoreform.css" rel="stylesheet" />
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue');//主键
    $(function () {
        InitControl();

    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/ZhangCeModule/DStudentTestScore/GetDStudentTestScoreModel", { studentcode: KeyValue }, function (data) {
                $("#heigh").val(data.Heigh);
                $("#weight").val(data.Weight);
                $("#bmi").val(data.BMI);
                $("#bmiscore").val(data.BMIScore);
                $("#pulmonary").val(data.Pulmonary);
                $("#pulmonaryscore").val(data.PulmonaryScore);
                $("#fiftyrun").val(data.FiftyRun == null ? "" : data.FiftyRun.replace('.', ''));
                $("#fiftyrunscore").val(data.FiftyRunScore);
                $("#standjump").val(data.StandJump);
                $("#standjumpscore").val(data.StandJumpScore);
                $("#sitandreach").val(data.SitAndReach == null ? "" : data.SitAndReach.replace('.', ''));
                $("#sitandreachscore").val(data.SitAndReachScore);
                $("#eighthundred").val(data.EightHundred);
                $("#eighthundredscore").val(data.EightHundredScore);
                $("#eighthundredaddscore").val(data.EightHundredAddScore);
                $("#thousandrun").val(data.ThousandRun);
                $("#thousandrunscore").val(data.ThousandRunScore);
                $("#thousandrunaddscore").val(data.ThousandRunAddScore);
                $("#minsupination").val(data.MinSupination);
                $("#minsupinationscore").val(data.MinSupinationScore);
                $("#minsupinationaddscore").val(data.MinSupinationAddScore);
                $("#pullup").val(data.PullUp);
                $("#pullupscore").val(data.PullUpScore);
                $("#pullupaddscore").val(data.PullUpAddScore);
                $("#studenttruescore").val(data.StudentTrueScore);
                $("#testresult").val(data.TestResult);

            });
            AjaxJson("/ZhangCeModule/DStudentTestScore/GetDStudentInfosModel", { studentcode: KeyValue }, function (data) {
                //alert(JSON.stringify(data));
                $("#gradecode").html(data.GradeCode);
                $("#schoolname").html(data.SchoolName);
                $("#schoolcode").html(data.SchoolCode);
                $("#classname").html(data.ClassName);
                $("#classcode").html(data.ClassCode);
                $("#studentname").html(data.StudentName);
                var sex = data.StudentSex == "1" ? "男" : "女";
                $("#studentsex").html(sex);
                $("#studentcode").html(data.StudentCode);

            });
        }
    }
    //保存事件
    function AcceptClick() {
        var studentname = $("#studentname").html();
        var studentsex = $("#studentsex").html() == "男" ? "1" : "2";
        var gradecode = $("#gradecode").html();
        var heigh = $("#heigh").val();
        var heighreg = /^\d+$/;
        if (heigh != "" && !heighreg.test(heigh)) {
            layer.alert("身高数据输入有误！");
            $("#heigh").val("");
            return false;
        }
        if (heigh == "") {
            heigh = "-1";
        }
        var weight = $("#weight").val();
        if (weight != "" && !heighreg.test(weight)) {
            layer.alert("体重数据输入有误！");
            $("#weight").val("");
            return false;
        }
        if (weight == "") {
            weight = "-1";
        }
        var pulmonary = $("#pulmonary").val();
        var preg = /^[0-9]$|^([1-9])([0-9]){0,3}$|^10000$/;
        if (pulmonary != "" && !preg.test(pulmonary)) {
            layer.alert("肺活量数据输入有误！");
            $("#pulmonary").val("");
            return false;
        }
        if (pulmonary == "") {
            pulmonary = "-1";
        }

        var fiftyrun = $("#fiftyrun").val();
        var regf = /^\d+$/;
        if (fiftyrun != "" && !regf.test(fiftyrun)) {
            layer.alert("50米跑数据输入有误！5.0s应输50");
            $("#fifty").val("");
            return false;
        }
        else {
            if (fiftyrun.length > 0 && fiftyrun.length < 2) {
                layer.alert("50米跑数据输入有误！如：5.0s应输50");
                return false;
            }
            else if (fiftyrun == "") {
                fiftyrun = "-1";
            } else {
                var sf = fiftyrun.substring(0, fiftyrun.length - 1) + "." + fiftyrun.substring(fiftyrun.length - 1, fiftyrun.length);
                fiftyrun = sf;
            }

        }

        if (fiftyrun == "") {
            fiftyrun = "-1";
        }
        if (parseInt(fiftyrun) > 0 && parseInt(fiftyrun) < 6) {
            layer.alert("50米跑数据超出人类极限请重新输入！");
            $("#fiftyrun").val("");
            return false;
        }

        var standjump = $("#standjump").val();
        var regj = /^\d+$/;
        if (standjump != "" && !regj.test(standjump)) {
            layer.alert("立定跳远数据输入有误！");
            $("#standjump").val("");
            return false;
        }


        if (standjump == "") {
            standjump = "-1";
        }

        var sitandreach = $("#sitandreach").val();
        var regs = /^-?\d+\.?\d{0,1}$/;
        if (sitandreach != "" && !regs.test(sitandreach)) {
            layer.alert("坐位体前屈数据输入有误！");
            $("#sit").val("");
            return false;
        }
        else {
            if (sitandreach.length < 2 && sitandreach.length > 0) {
                layer.alert("坐位体前屈数据输入有误！如：1.2cm应输12");
                return false;
            }
            else if (sitandreach == "") {
                sitandreach = "-1000";
            } else {
                var sf = sitandreach.substring(0, sitandreach.length - 1) + "." + sitandreach.substring(sitandreach.length - 1, sitandreach.length);

                sitandreach = sf;
            }
        }
        if (sitandreach == "") {
            sitandreach = "-1000";
        }

        var eighthundred = $("#eighthundred").val();

        var rege = /^\d{3,4}$/;
        if (eighthundred != "" && !rege.test(eighthundred)) {
            layer.alert("800米跑数据应为3-4位数字，输入有误！");
            $("#eighthundred").val("");
            return false;
        }
        if (parseInt(eighthundred.substring(eighthundred.length - 2, eighthundred.length)) >= 60) {
            layer.alert("800米数据输入有误！");
            $("#eighthundred").val("");
            return false;
        }
        if (eighthundred == "") {
            eighthundred = "-1";
        }

        var thousandrun = $("#thousandrun").val();
        var regt = /^\d{3,4}$/;;
        if (thousandrun != "" && !regt.test(thousandrun)) {
            layer.alert("1000米跑数据应为3-4位数字，输入有误！");
            return false;
        }
        if (parseInt(thousandrun.substring(thousandrun.length - 2, thousandrun.length)) >= 60) {
            layer.alert("1000米数据输入有误！");
            return false;
        }

        if (thousandrun == "") {
            thousandrun = "-1";
        }

        var minsupination = $("#minsupination").val();
        var regm = /^(?:0|[1-9][0-9]?|100)$/;
        if (minsupination != "" && !regm.test(minsupination)) {
            layer.alert("一分钟仰卧起坐数据输入有误！");
            return false;
        }

        if (minsupination == "") {
            minsupination = "-1";
        }

        var pullup = $("#pullup").val();
        var regp = /^(?:0|[1-9][0-9]?|100)$/;
        if (pullup != "" && !regp.test(pullup)) {
            layer.alert("引体向上数据输入有误！");
            return false;
        }

        if (pullup == "") {
            pullup = "-1";
        }

        var studentcode = $("#studentcode").html();

        var postData = {
            "studentname": studentname, "studentsex": studentsex, "gradecode": gradecode, "heigh": heigh, "weight": weight, "pulmonary": pulmonary, "fiftyrun": fiftyrun,
            "standjump": standjump, "sitandreach": sitandreach, "eighthundred": eighthundred, "thousandrun": thousandrun, "minsupination": minsupination, "pullup": pullup, "studentcode": studentcode
        }

        AjaxJson("/ZhangCeModule/DStudentTestScore/UpdateStudentScore", postData, function (data) {
            tipDialog(data.Message, 3, data.Code);
            top.frames[tabiframeId()].windowload();
            closeDialog();
        });
    }




