    function truetest(studentsex) {
        var bmiscore = $("#bmiscore").val();
        bmiscore = bmiscore == "" ? "0" : bmiscore;
        var pulmonaryscore = $("#pulmonaryscore").val();
        pulmonaryscore = pulmonaryscore == "" ? "0" : pulmonaryscore;
        var fiftyrunscore = $("#fiftyrunscore").val();
        fiftyrunscore = fiftyrunscore == "" ? "0" : fiftyrunscore;
        var standjumpscore = $("#standjumpscore").val();
        standjumpscore = standjumpscore == "" ? "0" : standjumpscore;
        var sitandreachscore = $("#sitandreachscore").val();
        sitandreachscore = sitandreachscore == "" ? "0" : sitandreachscore;
        var eighthundredscore = $("#eighthundredscore").val();
        eighthundredscore = eighthundredscore == "" ? "0" : eighthundredscore;
        var eighthundredaddscore = $("#eighthundredaddscore").val() == "" ? "0" : $("#eighthundredaddscore").val();
        var thousandrunscore = $("#thousandrunscore").val();
        thousandrunscore = thousandrunscore == "" ? "0" : thousandrunscore;
        var thousandrunaddscore = $("#thousandrunaddscore").val() == "" ? "0" : $("#thousandrunaddscore").val();
        var minsupinationscore = $("#minsupinationscore").val();
        minsupinationscore = minsupinationscore == "" ? "0" : minsupinationscore;
        var minsupinationaddscore = $("#minsupinationaddscore").val() == "" ? "0" : $("#minsupinationaddscore").val();
        var pullupscore = $("#pullupscore").val();
        pullupscore = pullupscore == "" ? "0" : pullupscore;
        var pullupaddscore = $("#pullupaddscore").val() == "" ? "0" : $("#pullupaddscore").val();
        var trueTest = "";
        if (studentsex == "1") {
            trueTest = parseFloat(parseInt(bmiscore) * 0.15 + parseInt(pulmonaryscore) * 0.15 + parseInt(fiftyrunscore) * 0.2 + parseInt(standjumpscore) * 0.1 + parseInt(sitandreachscore) * 0.1 + (parseInt(thousandrunscore) > 100 ? 100 : parseInt(thousandrunscore)) * 0.2 + (parseInt(pullupscore) > 100 ? 100 : parseInt(pullupscore)) * 0.1 + parseInt(thousandrunaddscore) + parseInt(pullupaddscore)).toString();
            $("#studenttruescore").val(trueTest);
        }
        if (studentsex == "2") {
            trueTest = parseFloat(parseInt(bmiscore) * 0.15 + parseInt(pulmonaryscore) * 0.15 + parseInt(fiftyrunscore) * 0.2 + parseInt(standjumpscore) * 0.1 + parseInt(sitandreachscore) * 0.1 + (parseInt(eighthundredscore) > 100 ? 100 : parseInt(eighthundredscore)) * 0.2 + (parseInt(minsupinationscore) > 100 ? 100 : parseInt(minsupinationscore)) * 0.1 + parseInt(eighthundredaddscore) + parseInt(minsupinationaddscore)).toString();
            $("#studenttruescore").val(trueTest);
        }


    }


