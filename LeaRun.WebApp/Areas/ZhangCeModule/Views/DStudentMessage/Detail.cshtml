@{
    ViewBag.Title = "基本城市表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/DBase_Cities/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>ID：</th>
            <td class='formValue'>
                <label id="ID" />
            </td>
            <th class='formTitle'>城市编号：</th>
            <td class='formValue'>
                <label id="CityCode" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>城市名称：</th>
            <td class='formValue'>
                <label id="CityName" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>更改时间：</th>
            <td class='formValue'>
                <label id="UpdateTime" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>
