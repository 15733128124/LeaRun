@{
    ViewBag.Title = "DStudent_TestAllot》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/DStudent_TestAllot/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>ID：</th>
            <td class='formValue'>
                <label id="ID" />
            </td>
            <th class='formTitle'>TestSetID：</th>
            <td class='formValue'>
                <label id="TestSetID" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>TestGroup：</th>
            <td class='formValue'>
                <label id="TestGroup" />
            </td>
            <th class='formTitle'>GroupCount：</th>
            <td class='formValue'>
                <label id="GroupCount" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>BeginPraDate：</th>
            <td class='formValue'>
                <label id="BeginPraDate" />
            </td>
            <th class='formTitle'>BeginWeek：</th>
            <td class='formValue'>
                <label id="BeginWeek" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>BeginPraTime：</th>
            <td class='formValue'>
                <label id="BeginPraTime" />
            </td>
            <th class='formTitle'>TestTeacher：</th>
            <td class='formValue'>
                <label id="TestTeacher" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>CreateTime：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
            <th class='formTitle'>UpdateTime：</th>
            <td class='formValue'>
                <label id="UpdateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
