@{
    ViewBag.Title = "班级表（随年份建表）》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/DStudent_Class/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="ID" />
            </td>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="GradeCode" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="SchoolCode" />
            </td>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="SchoolName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="ClassCode" />
            </td>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="ClassName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="UpdateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
